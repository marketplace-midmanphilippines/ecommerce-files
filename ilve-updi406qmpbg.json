h scope=\\\"row\\\">Highest Element Output</th><td>3200 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N1</th><td>2500 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N2</th><td>1800 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N3</th><td>3200 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N4</th><td>2500 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N5</th><td>3200 Watts</td></tr><tr><th scope=\\\"row\\\">Burner/Element Output N6</th><td>1800 Watts</td></tr><tr><th scope=\\\"row\\\"><br/><b>Oven</b></th><td></td></tr><tr><th scope=\\\"row\\\">Oven Control Type</th><td>Knobs</td></tr><tr><th scope=\\\"row\\\">Oven Door Type</th><td>Triple Glass Door</td></tr><tr><th scope=\\\"row\\\">Oven Cleaning Type</th><td>Easy Clean Enamel Cavities</td></tr><tr><th scope=\\\"row\\\">Main Oven Type</th><td>UOV 80 M</td></tr><tr><th scope=\\\"row\\\">Interior Width</th><td>25 3/8\\u2033</td></tr><tr><th scope=\\\"row\\\">Interior Height</th><td>14 3/8\\u2033</td></tr><tr><th scope=\\\"row\\\">Interior Depth</th><td>16 1/8\\u2033</td></tr><tr><th scope=\\\"row\\\">Oven Racks</th><td>2</td></tr><tr><th scope=\\\"row\\\"><br/><b>Technical Details</b></th><td></td></tr><tr><th scope=\\\"row\\\">Voltage</th><td>240</td></tr><tr><th scope=\\\"row\\\">Amps</th><td>63.3</td></tr><tr><th scope=\\\"row\\\"><br/><b>Certifications</b></th><td></td></tr><tr><th scope=\\\"row\\\">ADA Compliant</th><td>No</td></tr><tr><th scope=\\\"row\\\">Star-K Certified</th><td>No</td></tr><tr><th scope=\\\"row\\\"><br/><b>Warranty</b></th><td></td></tr><tr><th scope=\\\"row\\\">Warranty</th><td>2 Year Parts and Labor</td></tr><tr><th scope=\\\"row\\\"><br/><b>Features</b></th><td></td></tr><tr><th scope=\\\"row\\\">Sabbath Mode</th><td>No</td></tr><tr><th scope=\\\"row\\\">Display Type</th><td>LCD Display</td></tr><tr><th scope=\\\"row\\\">Drawer</th><td>Storage Drawer</td></tr><tr><th scope=\\\"row\\\">Timer</th><td>Yes</td><